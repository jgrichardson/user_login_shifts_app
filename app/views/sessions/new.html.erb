<h1>Sign In</h1>
<%= form_with url: session_path do |f| %>
    <p>
        <%= f.label 'user_name:'%><br>
        <%= f.text_field :user_name, id: 'user_name' %>
    </p>
    <p>
        <%= f.label 'password:'%><br>
        <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
    <%= f.submit 'Log In' %>
    </p>
<% end %>